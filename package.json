{
  "name": "linuxt",
  "version": "2.8.11",
  "description": "liNuxt is a builder SSR pages with Lit-Elements",
  "type": "module",
  "main": "./lib/grelinuxtet.cjs",
  "bin": {
    "linuxt": "./bin/index.cjs"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "run-p format:*",
    "format:eclint": "git ls-files | xargs eclint fix",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "prettier \"*\" --write --ignore-path .eslintignore",
    "lint": "run-p lint:*",
    "lint:eclint": "git ls-files | xargs eclint check",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier \"*\" --check --ignore-path .eslintignore",
    "release": "standard-version",
    "test": "mocha test",
    "test:watch": "npm test -- --watch"
  },
  "author": "manufosela",
  "license": "Apache-2.0",
  "dependencies": {
    "@web/dev-server": "^0.1.10",
    "puppeteer": "^8.0.0",
    "shelljs": "^0.8.4",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@open-wc/eslint-config": "^4.2.0",
    "chai": "^4.3.4",
    "eclint": "^2.8.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-lit": "^1.3.0",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-wc": "^1.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">= 7.0.0"
  }
}
